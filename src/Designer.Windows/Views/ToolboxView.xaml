<Window
    x:Class="Designer.Windows.Views.ToolboxView"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:controls="clr-namespace:Designer.Windows.Controls"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:Designer.Windows.Views"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:mvvm="http://prismlibrary.com/"
    Title="工具栏"
    Width="400"
    Height="720"
    mvvm:ViewModelLocator.AutoWireViewModel="True"
    ResizeMode="NoResize"
    ShowInTaskbar="False"
    Topmost="True"
    mc:Ignorable="d">
    <TabControl Margin="10" Style="{StaticResource TabControlSliding}">
        <TabItem Header="点位列表">
            <controls:Positions />
        </TabItem>
        <TabItem Header="图表控件">
            <controls:Diagrams />
        </TabItem>
        <TabItem Header="配置属性">
            <controls:Settings DataContext="{Binding SettingsVM}" />
        </TabItem>
    </TabControl>
</Window>
